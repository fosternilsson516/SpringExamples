<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="view" class="com.example.blockmanager.View"/>

    <bean id ="foster" class="depInjection.Student">
        <constructor-arg name="studentName" value="Foster Nilsson"/>
        <constructor-arg name="age" value="22"/>
    </bean>
    <bean id="ellen" class="depInjection.Student">
        <constructor-arg name="studentName" value="Ellen Rekedal"/>
        <constructor-arg name="age" value="23"/>
    </bean>
</beans>